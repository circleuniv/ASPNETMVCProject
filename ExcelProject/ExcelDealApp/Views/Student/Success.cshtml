
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Success</title>
</head>
<body>
   <h3>List Students</h3>
    <table>
        <tr>
            <th>SID</th>
            <th>Years</th>
            <th>Term</th>
            <th>VacType</th>
            <th>ClassCounts</th>
        </tr>
        @foreach (var item in ViewBag.ListAttendRecord)
        {
        <tr>
            <td> @item.SID</td>
            <td> @item.Years</td>
            <td> @item.Term</td>
            <td> @item.VacType</td>
            <td> @item.ClassCounts</td>
        </tr>
        }
    </table>
</body>
</html>
